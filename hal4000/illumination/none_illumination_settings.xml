<?xml version="1.0" encoding="ISO-8859-1"?>
<illumination_settings>

  <control_modules>
    <control_module>
      <name>wheel1</name>
      <module>illumination.filterWheel</module>
      <class>FilterWheel</class>
    </control_module>

    <control_module>
      <name>aotf1</name>
      <module>illumination.aotf</module>
      <class>AOTF</class>
    </control_module>
    
    <control_module>
      <name>laser1</name>
      <module>illumination.laser</module>
      <class>laser</class>
    </control_module>

    <control_module>
      <name>daq1</name>
      <module>illumination.daq</module>
      <class>daq</class>
    </control_module>
  </control_modules>
  
  <channels>
    <channel>
      <description>750</description>
      <color type="rgb">255,0,0</color>
      <channel_id type="int">0</channel_id>
      
      <amplitude_modulation>
	<uses>wheel</uses>
	<display_normalized type="boolean">False</display_normalized>
      </amplitude_modulation>
      
      <digital_modulation>
	<uses>daq1</uses>
      </digital_modulation>
    </channel>
    
    <channel>
      <description>642</description>
      <color type="rgb">255,0,0</color>
      <channel_id type="int">1</channel_id>
      
      <amplitude_modulation>
	<uses>aotf1</uses>
	<display_normalized type="boolean">True</display_normalized>
	<parameters>
	  <amplitude>6500</amplitude>
	  <channel type="int">0</channel>
	  <off_frequency type="float">20.0</off_frequency>
	  <on_frequency type="float">91.22</on_frequency>
	</parameters>
      </amplitude_modulation>
      
      <analog_modulation>
	<uses>daq1</uses>
	<parameters>
	  <channel type="int">1</channel>
	  <max_voltage type="float">1.0</max_voltage>
	  <min_voltage type="float">0.0</min_voltage>
      </parameters>
      </analog_modulation>
      
      <digital_modulation>
	<uses>daq1</uses>
	<parameters>
	  <channel type="int">1</channel>
	</parameters>
      </digital_modulation>
    </channel>
  </channels>
</illumination_settings>
