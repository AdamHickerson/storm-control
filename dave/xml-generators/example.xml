<?xml version="1.0" encoding="ISO-8859-1"?>
<experiment>
  <loop name = "Valve Loop" variable_name = "i">
    <variable_command name="1"></variable_command>
    <loop name = "Movie Loop" variable_name = "j">	
 	<item name = "Movie 1"></item>
	<item name = "Movie 2"></item>
        <item name = "Movie 3"></item>
    </loop>
  </loop>
</experiment>

<item name = "Movie 1">
       <movie>
          <name>bleach</name>
          <length>20</length>
          <lock_target>0.0</lock_target>
          <progression>
	     <type>lockedout</type>
          </progression>
          <variable_command name = "2"></variable_command>
       </movie>
</item>

<item name = "Movie 2">
